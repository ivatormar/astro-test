---
const response = await fetch('https://jsonplaceholder.typicode.com/users');
const data = await response.json();
console.log(data);
---

<table>
    <th>Name</th>
    <th>User Name</th>
    <th>Email</th>
    <th>Company name</th>
    {
        data.map((user) => (
            <tr>
                <td>{user.name}</td>
                <td>{user.username}</td>
                <td>{user.email}</td>
                <td>{user.company.name}</td>
            </tr>
        ))
    }
</table>
<style>
    table{
        color: antiquewhite;
    }
    table th{
        text-align: start;
    }
</style>